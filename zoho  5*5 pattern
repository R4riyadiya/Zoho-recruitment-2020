#include <stdio.h>

int main()
{
    int n;
    int i,j=0;
    printf("\nenter the number:");
    scanf("%d",&n);
    for (i=0;i<n;i++){
        
        for(j=0;j<n;j++){
            
            if(j==0 || i==0){
                printf("%d ",n);
            }
            else if(i==1 || j==1)
            {
                printf("%d ",n-1);
            }
            else if(i==2 || j==2)
            {
                printf("%d ",n-2);
            }
            else if(i==3 || j==3)
            {
                printf("%d ",n-3);
            }
            else if(i==4 || j==4)
            {
                printf("%d ",n-4);
            }
        }
    printf("\n");   
    }
     
    
    return 0;
}
